<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<statements 
 xmlns="http://jkniv.sf.net/schema/sqlegance/statements" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://jkniv.sf.net/schema/sqlegance/statements
 http://jkniv.sf.net/schema/sqlegance/sqlegance-stmt.xsd">


  <insert id="test-default-autokey-insert" type="NATIVE">
    <autoGeneratedKey strategy="AUTO" properties="id" />
    insert into Author (username,password,email) values (#{username},#{password},#{email})
  </insert>

  <insert id="test-auto-autokey-insert" type="NATIVE">
    <autoGeneratedKey strategy="AUTO" properties="id"/>
    insert into Author (username,password,email) values (#{username},#{password},#{email})
  </insert>

  <insert id="test-sequence-autokey-insert" type="NATIVE">
    <autoGeneratedKey strategy="SEQUENCE" properties="id"/>
    insert into Author (username,password,email) values (#{username},#{password},#{email})
  </insert>
<!--
  <insert id="test-table-autokey-insert" type="NATIVE">
    <autoGeneratedKey strategy="TABLE" properties="id"/>
    insert into Author (username,password,email) values (#{username},#{password},#{email})
  </insert>
  -->

  <insert id="test-no-autokey-insert" type="NATIVE">
    insert into Author (username,password,email) values (#{username},#{password},#{email})
  </insert>

  <insert id="test-sequence-autokey-insert-with-query" type="NATIVE">
    <autoGeneratedKey strategy="SEQUENCE" properties="id">select myschema.mysequence.nextval from dual</autoGeneratedKey>
    insert into Author (username,password,email) values (#{username},#{password},#{email})
  </insert>

</statements>